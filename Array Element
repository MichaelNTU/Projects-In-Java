package array1;

/**
 *
 * @author mikeo
 */
public class Array1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int[] a1 = {10,20,30,40,50};
        for (int i = 0; i < a1.length; ++i)
            System.out.print(a1[i] + " ");
        System.out.println();
    }
    }

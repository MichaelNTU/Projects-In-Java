package array2;

/**
 *
 * @author mikeo
 */
public class Array2 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int[][] a2 = {{1,2}, {3,4}, {5,6}};
        for (int j = 0; j < a2.length; ++j) {
            for (int k = 0; k < a2[j].length; ++k)
                System.out.print(a2[j][k] + " ");
            System.out.println();
        }
    }
    
}
